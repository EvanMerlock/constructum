{
    "configurations": [
        {
            "name": "Next.js: debug server",
            "type": "node-terminal",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}/constructum-ui/",
            "localRoot": "${workspaceFolder}/constructum-ui/"
        },
        {
            "name": "Constructum-Server",
            "type": "lldb",
            "program": "${workspaceFolder}/target/debug/constructum-server.exe",
            "request": "launch",
            "envFile": "${workspaceFolder}/.env"
        }
    ]

}